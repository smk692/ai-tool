# Qdrant Vector DB
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Notion API
NOTION_API_KEY=your_notion_api_key_here

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Slack
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_APP_TOKEN=xapp-your-app-token
SLACK_SIGNING_SECRET=your_signing_secret

# Embedding Model
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# MCP Servers
# Grafana
GRAFANA_URL=http://localhost:3000
GRAFANA_SERVICE_ACCOUNT_TOKEN=your_grafana_token

# Sentry
SENTRY_ACCESS_TOKEN=your_sentry_token

# AWS
AWS_PROFILE=default

# Swagger MCP
SWAGGER_MCP_JAR_PATH=/path/to/swagger-mcp.jar

# Jira MCP (@aashari/mcp-server-atlassian-jira)
ATLASSIAN_SITE_NAME=your-site-name
ATLASSIAN_USER_EMAIL=your_email@example.com
ATLASSIAN_API_TOKEN=your_atlassian_api_token

# Notion MCP (mcp-remote, OAuth 인증 사용)
